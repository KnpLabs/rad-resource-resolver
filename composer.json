{
    "name": "knplabs/rad-resource-resolver",
    "description": "A routing resource resolver based on conventions",
    "license": "MIT",
    "authors": [
        {
            "name": "KNP Labs",
            "email": "hello@knplabs.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/config": "~2.8 || ~3.0",
        "symfony/dependency-injection": "~2.8 || ~3.0",
        "symfony/event-dispatcher": "~2.8 || ~3.0",
        "symfony/http-foundation": "~2.8 || ~3.0",
        "symfony/http-kernel": "~2.8 || ~3.0",
        "symfony/routing": "~2.8 || ~3.0"
    },
    "require-dev": {
        "pedrotroller/php-cs-custom-fixer": "~1.2.1",
        "phpspec/phpspec": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "Knp\\Rad\\ResourceResolver\\": "src/Knp/Rad/ResourceResolver"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
